{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    ".classpath": true,
    ".project": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "wpilib.online": true,
  "files.associations": {
    "system_error": "cpp",
    "xtr1common": "cpp",
    "type_traits": "cpp",
    "xlocale": "cpp",
    "limits": "cpp",
    "string": "cpp"
  },
  "cSpell.language": "en-GB"
}
